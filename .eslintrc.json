{
    "env": {
        "browser": true,
        "es6": true,
        "node" : true
    },
    "extends": [
        "standard"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {                
        "accessor-pairs" : 0,
        "no-undef" : 0,
        "no-prototype-builtins" : 0,
        "no-useless-escape" : 0
    }
}
